{
  "name": "draft-markup",
  "version": "0.0.0",
  "description": "Pipeline for using markup input (for example Markdown) with Draft-js",
  "main": "lib/index.js",
  "scripts": {
    "test": "export TESTING=true; mocha --reporter spec --bail",
    "example-build": "browserify -t [ babelify --presets [ es2015 react ] ] -t [ stringify --extensions [.md] ] ./example/example.js > ./example/dist.js; lessc ./example/example.less ./example/dist.css",
    "example-deploy": "npm run example-build; gh-pages -d ./example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitbookIO/draft-markup.git"
  },
  "keywords": [
    "draft-js",
    "markdown",
    "react"
  ],
  "author": "Samy Pesse <samy@gitbook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GitbookIO/draft-markup/issues"
  },
  "homepage": "https://github.com/GitbookIO/draft-markup#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "draft-js": "^0.2.2",
    "eslint": "^2.4.0",
    "gh-pages": "^0.11.0",
    "less": "^2.6.1",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "reset-css": "^2.0.2011012603",
    "should": "^8.2.2"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "extend": "^3.0.0",
    "immutable-js": "^0.3.1-6",
    "is": "^3.1.0",
    "kramed": "^0.5.5",
    "lodash.find": "^4.2.0",
    "stringify": "^5.1.0"
  }
}
