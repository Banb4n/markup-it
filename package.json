{
  "name": "markup-it",
  "version": "1.0.0-pre.0",
  "description": "Pipeline for working with markup input (for example Markdown)",
  "main": "lib/index.js",
  "scripts": {
    "test": "export TESTING=true; mocha --inline-diffs --reporter spec ./test/all.js",
    "test-fixture": "export TESTING=true; mocha --reporter spec ./test/all.js --testOnlyFixture=$FIXTURE",
    "test-overview": "export TESTING=true; mocha --reporter '../../../test/reporter-overview' ./test/all.js || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitbookIO/draft-markup.git"
  },
  "keywords": [
    "draft-js",
    "markdown",
    "asciidoc",
    "gitbook"
  ],
  "author": "Samy Pesse <samy@gitbook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GitbookIO/markup-it/issues"
  },
  "homepage": "https://github.com/GitbookIO/markup-it#readme",
  "devDependencies": {
    "eslint": "^2.4.0",
    "mocha": "^2.4.5",
    "should": "^8.2.2",
    "should-html": ">=1.0.2",
    "util": "^0.10.3"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "html-entities": "^1.2.0",
    "htmlparser2": "^3.9.0",
    "immutable": "^3.7.6",
    "is": "^3.1.0",
    "object-values": "^1.0.0",
    "range-utils": "^1.1.0",
    "yargs": "^4.7.1"
  },
  "bin": {
    "markup-toJSON": "./bin/toJSON.js",
    "markup-toText": "./bin/toText.js",
    "markup-toHTML": "./bin/toHTML.js",
    "markup-toDraft": "./bin/toDraft.js",
    "markup-toMarkdown": "./bin/toMarkdown.js",
    "markup-lint": "./bin/lint.js"
  }
}
